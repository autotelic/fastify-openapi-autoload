  },
  "devDependencies": {
    "@autotelic/fastify-injector": "^0.2.0",
    "fastify": "^4.25.2",
    "standard": "^17.1.0",
    "tap": "^18.6.1",
    "tsd": "^0.30.3",
    "typescript": "^5.3.3"
  },
